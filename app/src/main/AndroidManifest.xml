<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.course_project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Course_project">
        <activity
            android:name=".Activities.DeliveryActivity"
            android:exported="false" />
        <activity
            android:name=".Activities.CardActivity"
            android:exported="false" />
        <activity
            android:name=".Activities.MoneyActivity"
            android:exported="false" />
        <activity
            android:name=".Activities.NatureActivity"
            android:exported="false" />
        <activity
            android:name=".Activities.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.SupportActivity"
            android:exported="false" />
        <activity
            android:name=".Activities.CartListActivity"
            android:parentActivityName=".Activities.MainActivity" />
        <activity android:name=".Activities.ShowDetailActivity" />
        <activity android:name=".Activities.MainActivity" />
    </application>

</manifest>